<?xml version="1.0" encoding="UTF-8"?>
<Installer>
    <Name>Raptor</Name>
    <Version>2022.12</Version>
    <Title>Raptor Installation</Title>
    <TitleColor>#FFFFFF</TitleColor>
    <ProductUrl>https://rapidsilicon.com/</ProductUrl>
    <Publisher>RapidSilicon</Publisher>
    <WizardShowPageList>true</WizardShowPageList>
    <PageListPixmap>installer_application_icon.png</PageListPixmap>
    <StartMenuDir>Raptor</StartMenuDir>
    <Logo>installer_logo.png</Logo>
    <WizardStyle>Classic</WizardStyle>
    <WizardMinimumHeight>35em</WizardMinimumHeight>
    <WizardMinimumWidth>70em</WizardMinimumWidth>
    <ProductImages>
        <ProductImage>
            <Image>image_slide2.png</Image>
            <Url>https://rapidsilicon.com/</Url>
        </ProductImage>
    </ProductImages>
    <StyleSheet>style.qss</StyleSheet>
    <SupportsModify>false</SupportsModify>
    <MaintenanceToolName>Uninstall</MaintenanceToolName>
    <TargetDir>@ApplicationsDir@/@Publisher@/@ProductName@/@ProductVersion@</TargetDir>
</Installer>
